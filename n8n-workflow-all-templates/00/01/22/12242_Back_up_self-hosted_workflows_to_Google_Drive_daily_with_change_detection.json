      "parameters": {
        "jsCode": "const errors = [];\nconst items = $input.all();\nconst n8nHost = items[0].json.n8nHost;\nconst apiKey = items[0].json.apiKey;\nconst folderPath = items[0].json.backupFolder;\nconst dtName = items[0].json.dataTableTitle;\nconst backupScope = items[0].json.backupScope;\nconst requiredTag = items[0].json.requiredTag;\n\n\nif (n8nHost == '') {\n  errors.push('A valid n8n hostname must be configured.');\n}\nif (apiKey == '') {\n  errors.push('A valid n8n ApiKey must be configured.');\n}\nif (folderPath == '') {\n  errors.push('Google drive directory path must be configured.');\n}\nif (dtName == '') {\n  errors.push('N8n data table name must be configured.');\n}\nif (backupScope == '') {\n  errors.push('The backup scope must be configured.');\n}\nif (backupScope == 'tagged' && requiredTag == '') {\n  errors.push('Please specify the tag name if you have configured the backup scope as “tagged”.');\n}\n\nif (errors.length) {\n  throw new Error(`Validation failed: ${errors.map((e, i) => `${i+1}. ${e}`).join(' | ')}`);\n}\n\nreturn items;"
