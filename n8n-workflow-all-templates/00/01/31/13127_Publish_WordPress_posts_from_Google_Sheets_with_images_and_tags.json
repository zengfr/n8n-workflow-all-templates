      "name": "Store tag_id",
      "type": "n8n-nodes-base.code",
      "position": [
        2000,
        432
      ],
      "parameters": {
        "jsCode": "const data = $getWorkflowStaticData('global');\n\nconst nextId = $json.id; // или $json.tag_id — что у тебя реально является ID\nconst idStr = String(nextId || '').trim();\n\nif (/^\\d+$/.test(idStr)) {\n  const current = String(data.wp_tag_ids || '0');\n  const parts = current.split(',').map(s => s.trim()).filter(Boolean);\n\n  // без дублей\n  if (!parts.includes(idStr)) {\n    data.wp_tag_ids = current + ',' + idStr;\n  }\n}\n\nreturn items;\n"
      },
      "typeVersion": 2
    },
