        "jsCode": "// ã‚ªãƒ•ã‚¡ãƒ¼ã‚¿ã‚¤ãƒ—ã®æ±ºå®š\nfor (const item of $input.all()) {\n  const mrr = item.json.mrr || 0;\n  const ticketCount = item.json.open_ticket_count || 0;\n  const loginDays = item.json.login_days_last_30d || 0;\n  \n  let offerType, offerValue;\n  \n  if (mrr > 10000) {\n    offerType = 'premium_package';\n    offerValue = '20% OFF + å„ªå…ˆã‚µãƒãƒ¼ãƒˆ + ç„¡æ–™ã‚¢ãƒƒãƒ—ã‚°ãƒ¬ãƒ¼ãƒ‰';\n  } else if (ticketCount > 5) {\n    offerType = 'support_upgrade';\n    offerValue = 'å„ªå…ˆã‚µãƒãƒ¼ãƒˆ + 10% OFF';\n  } else if (loginDays < 5) {\n    offerType = 'engagement_boost';\n    offerValue = 'å€‹åˆ¥ã‚ªãƒ³ãƒœãƒ¼ãƒ‡ã‚£ãƒ³ã‚° + 15% OFF';\n  } else {\n    offerType = 'standard_discount';\n    offerValue = '10% OFF for 3 months';\n  }\n  \n  item.json.offer_type = offerType;\n  item.json.offer_value = offerValue;\n  item.json.offer_expire_at = new Date(Date.now() + 14 * 24 * 60 * 60 * 1000).toISOString();\n}\n\nreturn $input.all();"
