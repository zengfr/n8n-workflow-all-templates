      ],
      "parameters": {
        "jsCode": "const keyword = $input.first().json.keyword;\nconst description = $input.first().json.description;\n\nconst dorky = [\n  `\"${keyword}\" music`,\n  `\"${keyword}\" concert`,\n  `\"${keyword}\" festival`,\n  `\"${keyword}\" album`,\n  `\"${keyword}\" band`,\n  `(\"${keyword}\" OR \"${keyword.replace(' ', '')}\") AND (music OR concert OR festival OR release)`\n];\n\nconst bannedDomains = [\n  'spotify.com',\n  'open.spotify.com',\n  'music.apple.com',\n  'bandcamp.com',\n  'soundcloud.com',\n  'youtube.com',\n  'youtu.be',\n  'instagram.com',\n  'facebook.com',\n  'twitter.com',\n  'x.com',\n  'tiktok.com',\n  'linktr.ee'\n];\n\n// VraÅ¥ array - kaÅ¾dÃ½ dork jako samostatnÃ½ item\nreturn dorky.map(dork => ({\n  json: {\n    currentDork: dork,\n    keyword: keyword,\n    description: description,\n    bannedDomains: bannedDomains,\n    timeRange: 'pd'  // past day pro Brave API\n  }\n}));"
      },
      "typeVersion": 2
    },
