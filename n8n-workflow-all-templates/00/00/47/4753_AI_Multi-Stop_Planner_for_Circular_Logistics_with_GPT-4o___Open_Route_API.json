        1580,
        700
      ],
      "parameters": {
        "jsCode": "const steps = items[0].json.routes[0].steps;\n\nconst orderedJobs = steps\n  .filter(step => step.type === \"job\")\n  .map((step, index) => ({\n    id: step.job,\n    sequence: index + 1\n  }));\n\nreturn orderedJobs.map(job => ({ json: job }));\n"
      },
      "typeVersion": 2
