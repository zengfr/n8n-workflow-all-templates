    {
      "id": "d63fd3cc-2966-4460-8de0-8b871d6f2e78",
      "name": "Sticky Note5",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        -3248,
        736
      ],
      "parameters": {
