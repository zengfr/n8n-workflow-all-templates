    {
      "id": "5674a7e0-1aa3-4085-acef-c41d22530140",
      "name": "Website Summarizer",
      "type": "@n8n/n8n-nodes-langchain.agent",
      "onError": "continueErrorOutput",
      "maxTries": 2,
      "position": [
        1920,
