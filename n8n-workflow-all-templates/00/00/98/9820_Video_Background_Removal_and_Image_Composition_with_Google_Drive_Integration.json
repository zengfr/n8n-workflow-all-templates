            },
            {
              "name": "background_image_url",
              "type": "string",
              "value": "={{ $json.body?.background_image_url ?? $json.background_image_url ?? 'https://drive.google.com/uc?id=1DYJxUcuN5n4BT4YZnRKYmXuLNSCs5bPy' }}"
            },
            {
              "name": "source",
