      ],
      "parameters": {
        "jsCode": "const input = items[0].json;\nconst firstCompany = input.companies?.[0];\n\nreturn [\n  {\n    json: firstCompany\n  }\n];"
      },
      "typeVersion": 2
    },
    {
      "id": "ac8c797e-0dc5-4f7c-9bf0-96e57943e606",
