              "operator": {
                "name": "filter.operator.equals",
                "type": "string",
                "operation": "equals"
              },
              "leftValue": "={{ $json.lead_source || $json.source }}",
              "rightValue": "SurveyMonkey"
            }
          ]
        }
      },
      "typeVersion": 2
    },
