        "options": {},
        "jsonBody": "={\n  \"model\": \"recraft-v3\",\n  \"input\": {\n    \"prompt\": \"{{ $json.message.content.image_prompt}}\", \n    \"style\": \"any\"\n  }\n}",
        "sendBody": true,
        "specifyBody": "json",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth"
      },
      "credentials": {
