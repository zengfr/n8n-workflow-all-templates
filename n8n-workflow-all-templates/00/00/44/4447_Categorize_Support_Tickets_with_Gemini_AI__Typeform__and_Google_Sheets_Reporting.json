      "parameters": {
        "jsCode": "const items = $input.all();\nconst counts = {};\nitems.forEach(item => {\n  const cat = item.json.Category;\n  if (cat) {\n    counts[cat] = (counts[cat] || 0) + 1;\n  }\n});\nreturn [{ json: { summary: counts } }];"
