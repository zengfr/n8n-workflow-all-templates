      "parameters": {
        "jsCode": "return $input.all().map(item => {\n  const isoTime = item.json.time;\n\n  const date = new Date(isoTime).toLocaleString('en-US', {\n    timeZone: 'Asia/Kolkata',  // üîÅ Change this to your UAT timezone\n    year: 'numeric',\n    month: 'long',\n    day: 'numeric',\n    hour: 'numeric',\n    minute: '2-digit',\n    hour12: true\n  });\n\n  return {\n    json: {\n      ...item.json,\n      time: date\n    }\n  };\n});\n"
      },
      "typeVersion": 2
    },
