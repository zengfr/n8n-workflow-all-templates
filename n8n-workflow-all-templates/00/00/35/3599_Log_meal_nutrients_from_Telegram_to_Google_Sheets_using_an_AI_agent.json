      ],
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "let entry = $input.item.json.output\nlet my_date = new Date()\n\nlet my_date_f = (my_date.getTime() / 86400000) + 25569;\nentry.my_date = my_date_f\nreturn {json: entry}"
      },
      "typeVersion": 2
    },
