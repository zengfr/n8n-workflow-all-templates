                "type": "string",
                "operation": "notRegex"
              },
              "leftValue": "={{ $json.snippet.title.toLowerCase() }}",
              "rightValue": "money|rich|profit|earn|cash|income|\\$|ðŸ’°|sell|agency|business|client|freelance|side.*hustle|make.*money"
            },
