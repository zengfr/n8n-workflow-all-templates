        1000
      ],
      "parameters": {
        "jsonSchemaExample": "{\n  \"Type\": \"Bug\",\n  \"Severity\": \"Important\",\n  \"Priority\": \"Medium\",\n  \"Status\": \"New\",\n  \"Missing\": \"Need more information about the fix. Where is it and what's the problem that is happening?\" \n  \n}"
